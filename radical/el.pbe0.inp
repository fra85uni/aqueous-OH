&FORCE_EVAL
  METHOD         Quickstep
  STRESS_TENSOR  ANALYTICAL
!  &PRINT
!    &STRESS_TENSOR
!   &END STRESS_TENSOR
!  &END PRINT
  &DFT
	    BASIS_SET_FILE_NAME /usr/local/cp2k-6.1/data/BASIS_MOLOPT
	    POTENTIAL_FILE_NAME /usr/local/cp2k-6.1/data/GTH_POTENTIALS
	    BASIS_SET_FILE_NAME /usr/local/cp2k-6.1/data/BASIS_ADMM_MOLOPT
	    BASIS_SET_FILE_NAME /usr/local/cp2k-6.1/data/BASIS_ADMM
            BASIS_SET_FILE_NAME /usr/local/cp2k-6.1/data/BASIS_RI_cc-TZ
	    WFN_RESTART_FILE_NAME /home/fambrosi/hydroxyl/test-koopman/radical/gga/el.pbe0/twater+I-.dyn-RESTART.wfn 
!    CHARGE +1
     UKS
    &MGRID
      CUTOFF       800
      REL_CUTOFF    50
    &END MGRID
    &QS
     METHOD GPW
     EPS_DEFAULT 1.0E-12
    &END QS
    &SCF
      SCF_GUESS  RESTART
      MAX_SCF    50
      &OT
        MINIMIZER       CG
        PRECONDITIONER  FULL_SINGLE_INVERSE
      &END
      &OUTER_SCF
        MAX_SCF  50
      &END
!      &PRINT
!        &RESTART OFF
!        &END
!      &END
    &END SCF
    &AUXILIARY_DENSITY_MATRIX_METHOD
        METHOD BASIS_PROJECTION
        ADMM_PURIFICATION_METHOD MO_DIAG
     &END AUXILIARY_DENSITY_MATRIX_METHOD
   &XC
    &XC_FUNCTIONAL
   &PBE
   SCALE_X 0.60
   SCALE_C 1.0
   &END PBE
   &PBE_HOLE_T_C_LR
    CUTOFF_RADIUS 6.0
    SCALE_X 0.40
    &END PBE_HOLE_T_C_LR
    &END XC_FUNCTIONAL
   &HF
    &SCREENING
    EPS_SCHWARZ 1.0E-6
    SCREEN_ON_INITIAL_P FALSE
    &END SCREENING
    &INTERACTION_POTENTIAL
     POTENTIAL_TYPE TRUNCATED
    CUTOFF_RADIUS 6.0
    T_C_G_DATA  /usr/local/cp2k-6.1/data/t_c_g.dat
    &END INTERACTION_POTENTIAL
     &MEMORY
     MAX_MEMORY 10000
    EPS_STORAGE_SCALING 0.1
    &END MEMORY
    FRACTION 0.40
    &END HF
      ! non-local vdW part
      &vdW_POTENTIAL
         DISPERSION_FUNCTIONAL NON_LOCAL
         &NON_LOCAL
           TYPE RVV10
           PARAMETERS  5.3  0.0093
           KERNEL_FILE_NAME   /usr/local/cp2k-6.1/data/rVV10_kernel_table.dat
           CUTOFF      400
         &END NON_LOCAL
      &END vdW_POTENTIAL
    &END XC
    &PRINT
     &MO_CUBES
      NHOMO 1
      NLUMO 1
      WRITE_CUBE 
!      &EACH  
!       MD 50
!      &END EACH
      &END MO_CUBES
   &E_DENSITY_CUBE
   &END 
   &END PRINT
  &END DFT
  &SUBSYS
    &CELL
      ABC [angstrom] 12.42 12.42 12.42	
    &END CELL
    &COORD


    &END COORD
!    &TOPOLOGY
!      COORD_FILE_NAME   ./pos74.xyz
!      COORD_FILE_FORMAT xyz
!    &END TOPOLOGY
!    &KIND Pb
!      BASIS_SET DZVP-MOLOPT-SR-GTH-q4
!      POTENTIAL GTH-PBE-q4
!    &END KIND
!    &KIND I
!      BASIS_SET DZVP-MOLOPT-SR-GTH-q7
!      POTENTIAL GTH-PBE-q7
!    &END KIND
    &KIND O
!      BASIS_SET DZVP-MOLOPT-SR-GTH-q6
     BASIS_SET AUX_FIT cFIT3
      BASIS_SET cc-TZ
      POTENTIAL GTH-PBE-q6
    &END KIND
    &KIND H
!      BASIS_SET DZVP-MOLOPT-SR-GTH-q1
      BASIS_SET AUX_FIT cFIT3
     BASIS_SET cc-TZ
      POTENTIAL GTH-PBE-q1
    &END KIND
    &KIND C
     BASIS_SET DZVP-MOLOPT-SR-GTH-q4
!      BASIS_SET cc-TZ
      POTENTIAL GTH-PBE-q4
      BASIS_SET AUX_FIT cFIT3 !SZV-MOLOPT-SR-GTH
    &END KIND
    &KIND N
!      BASIS_SET cc-TZ
      BASIS_SET DZVP-MOLOPT-SR-GTH-q5
      POTENTIAL GTH-PBE-q5
      BASIS_SET AUX_FIT cFIT3 !SZV-MOLOPT-SR-GTH
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&MOTION
 &MD
  STEPS	50000000
  TIMESTEP 0.48
  TEMPERATURE 350
  ENSEMBLE NVT
  &THERMOSTAT
